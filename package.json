{
  "private": true,
  "browserslist": "> 5%",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "test": "jest",
    "dev": "yarn run build:tsc && npm-run-all -p build:tsc:watch dev:start",
    "dev:start": "wsrun --exclude-missing --stages dev",
    "build": "npm-run-all build:tsc build:other",
    "build:tsc": "tsc --build tsconfig.json",
    "build:other": "wsrun --exclude-missing --stages build",
    "build:tsc:watch": "tsc --build tsconfig.json --watch"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.7.7",
    "@babel/plugin-proposal-class-properties": "^7.7.4",
    "@babel/preset-env": "^7.7.7",
    "@babel/preset-react": "^7.7.4",
    "@babel/preset-typescript": "^7.7.7",
    "@stylable/core": "^2.5.3",
    "@stylable/webpack-plugin": "^3.0.0",
    "@types/dotenv-webpack": "^1.7.0",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.0.25",
    "@types/webpack": "^4.41.1",
    "@types/webpack-dev-server": "^3.9.0",
    "@types/webpack-merge": "^4.1.5",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.13.0",
    "babel-loader": "^8.0.6",
    "dotenv-webpack": "^1.7.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "fork-ts-checker-webpack-plugin": "^3.1.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.5",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "stylable": "^5.4.11",
    "ts-node": "^8.5.4",
    "typescript": "^3.7.4",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "whatwg-fetch": "^3.0.0",
    "wsrun": "^5.2.0"
  }
}
